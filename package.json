{
  "name": "truffle-sveltekit-base",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "migrate": "truffle --config truffle-config.cjs migrate",
    "compile": "truffle --config truffle-config.cjs compile",
    "generate-types": "npx typechain --target=truffle-v5 'build/contracts/*.json'",
    "postinstall": "npx truffle compile && npm run generate-types"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "^4.4.1",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "tslib": "^2.3.1",
    "web3": "^1.6.1",
    "@typechain/truffle-v5": "^6.0.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.8",
    "truffle": "^5.4.27",
    "ts-node": "^10.4.0",
    "typechain": "^6.1.0",
    "typescript": "^4.5.4"
  },
  "type": "module",
  "dependencies": {
    "autoprefixer": "^10.4.2",
    "daisyui": "^1.22.0",
    "postcss-load-config": "^3.1.1",
    "svelte-web3": "^3.1.0",
    "tailwindcss": "^3.0.12"
  }
}
